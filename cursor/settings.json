{
    "window.commandCenter": false,
    "window.title": "${activeEditorShort}${separator}${rootName}${separator}${profileName}",
    "emacs-mcx.strictEmacsMove": true,
    "workbench.colorCustomizations": {
        "editor.background": "#202020",
        "editorWidget.background": "#202020",
        "panel.background": "#202020",
        "sideBar.background": "#202020",
        "activityBar.background": "#202020",
        "statusBar.background": "#202020",
        "editorGutter.background": "#202020",
        "editor.lineHighlightBackground": "#242424",
        "editorIndentGuide.background1": "#262626",
        "editorIndentGuide.activeBackground1": "#303030",
        "panel.border": "#202020",
        "sideBar.border": "#262626",
        "activityBar.border": "#262626",
        "statusBar.border": "#262626",
        "editorGroup.border": "#262626",
        "editorGutter.modifiedBackground": "#202020",
